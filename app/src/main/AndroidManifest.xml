<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dhw.musicplayer">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".AllMusicListActivity"
            android:label="@string/app_name"></activity>

        <activity
            android:name=".MusicPlayerActivity"
            android:label="@string/title_activity_first"></activity>
        <activity
            android:name="com.example.dhw.musicplayer.SplashActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name="com.example.dhw.musicplayer.MusicService"></service>
    </application>

</manifest>